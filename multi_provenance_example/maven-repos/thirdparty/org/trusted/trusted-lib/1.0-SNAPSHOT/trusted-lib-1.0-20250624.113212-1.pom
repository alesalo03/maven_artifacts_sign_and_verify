<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.trusted</groupId>
  <artifactId>trusted-lib</artifactId>
  <packaging>jar</packaging>
  <version>1.0-SNAPSHOT</version>
  <name>trusted-lib</name>
  <url>http://maven.apache.org</url>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
  </dependencies>

  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-jarsigner-plugin</artifactId>
        <version>3.0.0</version>
        <executions>
          <execution>
            <id>sign</id>
            <phase>verify</phase>
            <goals>
              <goal>sign</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <keystore>C:/Users/Alessandro/Documents/UNI/Tirocinio/multi_provenance_example/keystore.p12</keystore>
          <storepass>ppppppp</storepass>
          <keypass>ppppppp</keypass>
          <alias>myKey</alias>
          <storetype>PKCS12</storetype>
          <verbose>true</verbose>
        </configuration>
      </plugin>
    </plugins>
  </build>

  <distributionManagement>
    <repository>
      <id>maven-local-repository</id>
      <url>file:///C:/Users/Alessandro/Documents/UNI/Tirocinio/multi_provenance_example/maven-repos/thirdparty</url>
    </repository>
  </distributionManagement>
</project>
